-- classes

function create_attractor()
	randw=rnd(128)
	randh=rnd(128)
		pt={x=randw,y=randh} setmetatable(pt, vec2d)
	return {
		pt=pt,
	}
end

vec2d={
 __add=function(a,b)
  return {x=(a.x+b.x), y=(a.y+b.y)}
 end
}

