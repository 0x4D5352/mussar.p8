pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	poke(0x5f2d,0x1)
	string = {}
end

function _update()
	if stat(30) then
		add(string,stat(31))
	end
end

function _draw()
	cls()
	--cursor(stat(38),stat(39),7)
	--pset(stat(38),stat(39),7)
	print("kp? "..tostr(stat(30)))
	print("char: "..stat(31))
	print("mx: "..stat(32))
	print("my: "..stat(33))
	print("mbs: "..stat(34))
	print("mw: "..stat(36))
	pset(stat(32),stat(33),7)
	local res = ""
	if #string>=1 then
		for i=1,#string,1 do
			res = res..string[i]
		end
	end
	print(res,0,64,7)
end
